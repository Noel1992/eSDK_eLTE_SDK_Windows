<?xml version="1.0" encoding="utf-8"?>
<sca:svcagent xmlns:sca="http://www.td-tech.com/ubp/platform/sca/xml"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.td-tech.com/ubp/platform/sca/xml svc_agent_4.0.xsd"
         name="svc_agent">
         <!-- tcmalloc_trigger="on" ： open tcmalloc -->
         <!-- valgrind_trigger="on" ： open valgrind -->
    <ServiceInfo svc_type="hpgw_agent" code_type="java" user="ubp" team="BCC" tcmalloc_trigger="off" valgrind_trigger="off">
      <SvcParams>
        <Param name="start_java" value="/opt/UBP/bin/hpgw_webstart.sh"/>
        <Param name="pid_java" value="/opt/UBP/bin/hpgw_status.sh"/>
        <Param name="stop_java" value="/opt/UBP/bin/hpgw_webstop.sh"/>
        <Param name="init_wait_time" value="120"/>
      </SvcParams>
      <LoadModules>
      </LoadModules>
      <LoadPB>
      </LoadPB>
      <DependSvcs>
        <Svc name="cm_agent" />
      </DependSvcs>
    </ServiceInfo>
</sca:svcagent>