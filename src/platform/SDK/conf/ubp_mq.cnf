# This is for UBP MQ configuration.


# Transport layer security : 1 for open, 0 for not. Default is 0.
TLS = 1

#Domain
DOMAIN = 3

#login消息超时时间
LOGIN_TIMEOUT = 10

#服务端消息在队列中的等待时间,单位：秒
ASYN_RPN_WAIT_TIMEOUT = 120

#socket最大闲置时间（秒）,超过则会被关闭;0表示不关闭
MAX_IDLE_TIME = 0

#Socket TCP重连时间，单位：毫秒
RECONNECT_IVL = 2000

#最多socket数量（只在C++有效）
MAX_SOCKETS = 60000

#发布者和消费者心跳的时间，单位：秒
PUB_SUB_HEARTBEAT_INTERVAL = 60

#消费者心跳超时次数
SUBSCRIBE_TIMES = 3

#消费者心跳超时时间，单位：秒
SUBSCRIBE_TIMEOUT_TIME = 60

#服务端在回调处理消息超时时间,单位：秒
REPONSE_HANDLE_MQ_TIMEOUT = 60

#服务端处理消息回调的任务个数
MSGCALLBACK_WORKER_COUNT = 1

#客户方发送消息默认超时时间，单位ms
DEFAULT_REQUEST_TIMEOUT = 5000

#客户方异步消息Socket poll时间周期（只在Java版本有效了）
SOCKET_POLL_WAIT_TIME = 300000

#默认超时时间（删除）
DEFAULT_REQUEST_NOTACTIVE_TIMEOUT = 300000

#客户方默认发送次数
DEFAULT_REQUEST_RETRY_TIMES = 3

#是否支持2.1版本ZMQ接口
SUPPORT_VER21 = 0



