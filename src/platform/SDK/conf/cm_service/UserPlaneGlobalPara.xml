<?xml version="1.0" encoding="utf-8" ?>
<Moc-Config>
	<MOC MOC_NAME="UserPlaneGlobalPara" MOC_NAME_C="用户面全局参数" Module="BDC" ACCESS_CONTROL="modify,read" DEST_SVC="" Group="ParamCfgGrp" Relation="">
