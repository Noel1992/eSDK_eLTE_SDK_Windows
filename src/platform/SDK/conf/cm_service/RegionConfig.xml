		<Parameter name="SIPSecuritySwitch" desc="SIP加密开关" type="enum" range="0,1" vlist="0:不加密,1:加密" mode="display" />
		<Parameter name="RTPSecuritySwitch" desc="RTP加密开关" type="enum" range="0,1" vlist="0:不加密,1:加密" mode="display" />
	</MOC>
</Moc-Config>